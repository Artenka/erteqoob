    function philosophyIntroInit() {

      TweenMax.to($('.philosophy-intro__bg'), 3, {scaleX: 1.1, scaleY: 1.1, opacity: 0.73, ease: Power4.easeOut});
      TweenMax.to($('.philosophy-intro__logo'), 0.7, {opacity: 1, ease: Power1.easeOut, delay: 0.5});
      TweenMax.to($('.philosophy-intro__title h1'), 0.7, {top: 0, opacity: 1, ease: Power1.easeOut, delay: 0.3});
      TweenMax.to($('.philosophy-intro__title h2'), 0.7, {top: 0, opacity: 1, ease: Power1.easeOut, delay: 0.5});
      TweenMax.to($('.philosophy-intro__video'), 1, {top: 0, opacity: 1, ease: Power1.easeOut, delay: 1});
      TweenMax.to($('.scroll-down'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.5});
      if ($(window).width() >= 1260) {
        TweenMax.to($('.main-layout > .main-nav--socials'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.5});
      }
      TweenMax.to($('.philosophy-intro .circle-layout'), 0, {autoAlpha: 1, ease: Power4.easeOut});
      TweenMax.to($('.philosophy-intro .circle-line'), 0, {autoAlpha: 1, ease: Power4.easeOut});
      TweenMax.to($('.philosophy-intro .circle--3'), 0.4, {autoAlpha: 1, ease: Power4.easeOut});
      TweenMax.to($('.philosophy-intro .circle--2'), 0.4, {
        autoAlpha: 1,
        top: '9.5%',
        left: '9.5%',
        width: '81%',
        height: '81%',
        ease: Power4.easeOut,
        delay: 0.3
      });
      TweenMax.to($('.philosophy-intro .circle--1'), 0.4, {
        autoAlpha: 1,
        top: 0,
        left: 0,
        width: '100%',
        height: '100%',
        ease: Power4.easeOut,
        delay: 0.6
      });

      philosophyIntroReinit = false;
    }

    function philosophyIntroHide() {
      if (!philosophyIntroReinit) {

        TweenMax.to($('.philosophy-intro__bg'), 3, {scaleX: 1, scaleY: 1, opacity: 0, ease: Power4.easeOut});
        TweenMax.to($('.main-layout > .main-nav--socials'), 1, {opacity: 0, ease: Power4.easeOut});
        TweenMax.to($('.scroll-down'), 1, {opacity: 0, ease: Power4.easeOut});

        TweenMax.to($('.philosophy-intro__title h1'), 0.7, {top: '50px', opacity: 0, ease: Power1.easeOut, delay: 0.7});
        TweenMax.to($('.philosophy-intro__title h2'), 0.7, {top: '50px', opacity: 0, ease: Power1.easeOut, delay: 0.9});
        TweenMax.to($('.philosophy-intro .circle--3'), 0.7, {autoAlpha: 0, ease: Power4.easeOut});
        TweenMax.to($('.philosophy-intro .circle--2'), 0.7, {
          autoAlpha: 0,
          top: '19%',
          left: '19%',
          width: '62%',
          height: '62%',
          ease: Power4.easeOut,
          delay: 0.1
        });
        TweenMax.to($('.philosophy-intro .circle--1'), 0.7, {
          autoAlpha: 0,
          top: '9.5%',
          left: '9.5%',
          width: '81%',
          height: '81%',
          ease: Power4.easeOut,
          delay: 0.2
        });
        TweenMax.to($('.philosophy-intro .circle-layout'), 0, {autoAlpha: 0, ease: Power4.easeOut, delay: 0.9});
        TweenMax.to($('.philosophy-intro .circle-line'), 0, {autoAlpha: 0, ease: Power4.easeOut});

        philosophyIntroReinit = true;
      }
    }

