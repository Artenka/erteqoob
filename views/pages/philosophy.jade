    include ../partials/main-nav-socials.jade
    include ../partials/main-nav-toggle.jade
    include ../partials/register-link.jade
    div.scroll-layout
      include ../partials/philosophy/philosophy-intro.jade
      include ../partials/philosophy/philosophy-about-1.jade
      include ../partials/philosophy/philosophy-about-2.jade
      include ../partials/philosophy/philosophy-about-3.jade
      include ../partials/philosophy/philosophy-inspiration.jade
      include ../partials/philosophy/philosophy-pros-1.jade
      include ../partials/philosophy/philosophy-pros-2.jade
      //include ../partials/philosophy/philosophy-video.jade
      include ../partials/philosophy/philosophy-action.jade
      include ../partials/footer.jade

    include ../partials/philosophy/philosophy-inspiration-music.jade

  script.
    $(function () {
      fixedMenusInitAnimation();

      // hide fixed menus on mobile
      $(window).on('resize', function () {
        var window = $(this);
        if (window.width() < 1260) {
          $('.main').css('overflowY', 'auto');
          TweenMax.to($('.main-layout > .main-nav--cities'), 1, {opacity: 0, ease: Power1.easeOut});
          TweenMax.to($('.main-layout > .register-link'), 1, {opacity: 0, ease: Power1.easeOut});
          TweenMax.to($('.main-layout > .main-nav--socials'), 1, {opacity: 0, ease: Power1.easeOut});
          philosophyIntroInit();
          philosophyVideoInit();
          philosophyAbout1Init();
          philosophyAbout2Init();
          philosophyAbout3Init();
          philosophyInspirationInit();
          philosophyPros1Init();
          philosophyPros2Init();
          philosophyActionInit();
        } else {
          $('.main').css('overflowY', 'hidden');
          if ($('.onepage-pagination li .active').data('index') == 1) {
            TweenMax.to($('.main-layout > .main-nav--cities'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
            TweenMax.to($('.main-layout > .register-link'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
          }
        }
      });

      // init animations on mobile
      if ($(window).width() < 1260) {
        $('.main').css('overflowY', 'auto');
        philosophyIntroInit();
        philosophyVideoInit();
        philosophyAbout1Init();
        philosophyAbout2Init();
        philosophyAbout3Init();
        philosophyInspirationInit();
        philosophyPros1Init();
        philosophyPros2Init();
        philosophyActionInit();
      }

      // Onepage scroll init
      var pageScroll = ".scroll-layout";
      $(pageScroll).onepage_scroll({
        sectionContainer: "section",
        easing: "ease",
        animationTime: 1200,
        pagination: true,
        updateURL: false,
        beforeMove: function (index) {
          //if (index == 10) {
          //  fixedMenusHideAnimation();
          //} else {
          //  fixedMenusReinit();
          //}
          //hidePagePagination();

          if (index == 1) {
            hidePagePagination();
            philosophyVideoHide();
            philosophyAbout1Hide();
            philosophyAbout2Hide();
            philosophyAbout3Hide();
            philosophyInspirationHide();
            philosophyPros1Hide();
            philosophyPros2Hide();
            philosophyActionHide();
            philosophyIntroInit();
          } else if (index == 2) {
            philosophyIntroHide();
            philosophyVideoHide();
            showPagePagination();
            philosophyAbout2Hide();
            philosophyAbout3Hide();
            philosophyInspirationHide();
            philosophyPros1Hide();
            philosophyPros2Hide();
            philosophyActionHide();
            philosophyAbout1Init();
          } else if (index == 3) {
            philosophyIntroHide();
            philosophyVideoHide();
            showPagePagination();
            philosophyAbout1Hide();
            philosophyAbout3Hide();
            philosophyInspirationHide();
            philosophyPros1Hide();
            philosophyPros2Hide();
            philosophyActionHide();
            philosophyAbout2Init();
          } else if (index == 4) {
            philosophyIntroHide();
            philosophyVideoHide();
            showPagePagination();
            philosophyAbout1Hide();
            philosophyAbout2Hide();
            philosophyInspirationHide();
            philosophyPros1Hide();
            philosophyPros2Hide();
            philosophyActionHide();
            philosophyAbout3Init();
          } else if (index == 5) {
            philosophyIntroHide();
            philosophyVideoHide();
            showPagePagination();
            philosophyAbout1Hide();
            philosophyAbout2Hide();
            philosophyAbout3Hide();
            philosophyPros1Hide();
            philosophyPros2Hide();
            philosophyActionHide();
            philosophyInspirationInit();
          } else if (index == 6) {
            philosophyIntroHide();
            philosophyVideoHide();
            showPagePagination();
            philosophyAbout1Hide();
            philosophyAbout2Hide();
            philosophyAbout3Hide();
            philosophyInspirationHide();
            philosophyPros2Hide();
            philosophyActionHide();
            philosophyPros1Init();
          } else if (index == 7) {
            philosophyIntroHide();
            philosophyVideoHide();
            showPagePagination();
            philosophyAbout1Hide();
            philosophyAbout2Hide();
            philosophyAbout3Hide();
            philosophyInspirationHide();
            philosophyPros1Hide();
            philosophyActionHide();
            philosophyPros2Init();
          } else if (index == 8) {
            philosophyIntroHide();
            showPagePagination();
            philosophyAbout1Hide();
            philosophyAbout2Hide();
            philosophyAbout3Hide();
            philosophyInspirationHide();
            philosophyPros1Hide();
            philosophyPros2Hide();
            //philosophyActionHide();

            // temp
            philosophyVideoHide();

            //philosophyVideoInit();
            philosophyActionInit();
          } else if (index == 9) {
            philosophyIntroHide();
            philosophyVideoHide();
            showPagePagination();
            philosophyAbout1Hide();
            philosophyAbout2Hide();
            philosophyAbout3Hide();
            philosophyInspirationHide();
            philosophyPros1Hide();
            philosophyPros2Hide();

            //temp
            philosophyActionHide();

            //philosophyActionInit();
            footerMenusHideAnimation();
          //} else if (index == 10) {
          //  philosophyIntroHide();
          //  philosophyVideoHide();
          //  showPagePagination();
          //  philosophyAbout1Hide();
          //  philosophyAbout2Hide();
          //  philosophyAbout3Hide();
          //  philosophyInspirationHide();
          //  philosophyPros1Hide();
          //  philosophyPros2Hide();
          //  philosophyActionHide();
          //  footerMenusHideAnimation();
          }
        },
        afterMove: function(index) {

        },
        loop: false,
        keyboard: true,
        responsiveFallback: 1260,
        direction: "vertical"
      });

      // start intro animation on page load
      setTimeout(function () {
        if ($('.onepage-pagination li .active').data('index') == 1) {
          philosophyIntroInit();
        }
      }, 100);
    });

    var philosophyIntroReinit = true;
    var philosophyAbout1Reinit = true;
    var philosophyAbout2Reinit = true;
    var philosophyAbout3Reinit = true;
    var philosophyVideoReinit = true;
    var philosophyInspirationReinit = true;
    var philosophyPros1Reinit = true;
    var philosophyPros2Reinit = true;
