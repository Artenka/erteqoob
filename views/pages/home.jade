extends ../layouts/main-client.jade

block contents
  div.main-layout#home
    include ../partials/main-nav-cities.jade
    include ../partials/main-nav-socials.jade
    include ../partials/main-nav-lang.jade
    include ../partials/main-nav-toggle.jade
    include ../partials/register-link.jade
    .home__circle-layout
      .home__circle.home__circle--1
      .home__circle.home__circle--2
      .home__circle.home__circle--3
    div.scroll-layout
      include ../partials/home/home-intro.jade
      include ../partials/home/home-nav.jade
      include ../partials/home/home-think.jade
      include ../partials/home/home-video.jade
      include ../partials/home/home-about.jade
      include ../partials/home/home-founders.jade
      include ../partials/home/home-system.jade
      include ../partials/home/home-events.jade
      include ../partials/home/home-collections.jade
      include ../partials/home/home-blog.jade
      include ../partials/home/home-action.jade
      include ../partials/footer.jade



  script.
    $(function () {

      // hide fixed menus on mobile
      $(window).on('resize', function () {
        var window = $(this);
        if (window.width() < 640) {
          TweenMax.to($('.main-layout > .main-nav--cities'), 1, {opacity: 0, ease: Power1.easeOut});
          TweenMax.to($('.main-layout > .register-link'), 1, {opacity: 0, ease: Power1.easeOut});
          TweenMax.to($('.main-layout > .main-nav--socials'), 1, {opacity: 0, ease: Power1.easeOut});
          TweenMax.to($('.main-layout > .main-nav--lang'), 1, {opacity: 0, ease: Power1.easeOut});
          homeIntroInit();
          homeNavInit();
          homeThinkInit();
          homeVideoInit();
          homeAboutInit();
          homeFoundersInit();
          homeSystemInit();
          homeEventsInit();
          homeCollectionsInit();
          homeBlogInit();
          homeActionInit();
        } else {
          if ($('.onepage-pagination li .active').data('index') == 1) {
            TweenMax.to($('.main-layout > .main-nav--cities'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
            TweenMax.to($('.main-layout > .register-link'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
          }
        }
      });

      // init animations on mobile
      if ($(window).width() < 640) {
        homeIntroInit();
        homeNavInit();
        homeThinkInit();
        homeVideoInit();
        homeAboutInit();
        homeFoundersInit();
        homeSystemInit();
        homeEventsInit();
        homeCollectionsInit();
        homeBlogInit();
        homeActionInit();
      }

      // Onepage scroll init
      var pageScroll = ".scroll-layout";
      $(pageScroll).onepage_scroll({
        sectionContainer: "section",
        easing: "ease",
        animationTime: 1200,
        pagination: true,
        updateURL: false,
        beforeMove: function (index) {
          if (index == 2 || index == 12 ) {
            fixedMenusHideAnimation();
          } else {
            fixedMenusInitAnimation();
          }
          //hidePagePagination();

          if (index == 1) {
            hidePagePagination();
            homeNavHide();
            homeAboutHide();
            homeThinkHide();
            homeVideoHideFull();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeIntroInit();
          } else if (index == 2) {
            homeIntroHide();
            homeAboutHide();
            homeThinkHide();
            homeVideoHideFull();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeNavInit();
            showPagePagination();
          } else if (index == 3) {
            homeIntroHide();
            homeNavHide();
            homeAboutHide();
            homeVideoHideFull();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeThinkInit();
            showPagePagination();
          } else if (index == 4) {
            homeIntroHide();
            homeNavHide();
            homeAboutHide();
            homeThinkHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeVideoInit();
            showPagePagination();
          } else if (index == 5) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeAboutInit();
            showPagePagination();
          } else if (index == 6) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHide();
            homeAboutHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeFoundersInit();
            showPagePagination();
          } else if (index == 7) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHideFull();
            homeAboutHide();
            homeFoundersHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeSystemInit();
            showPagePagination();
          } else if (index == 8) {
            hidePagePagination();
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHideFull();
            homeAboutHide();
            homeFoundersHide();
            homeSystemHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeEventsInit();
          }  else if (index == 9) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHideFull();
            homeAboutHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeBlogHide();
            homeActionHide();
            homeCollectionsInit();
            showPagePagination();
          } else if (index == 10) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHideFull();
            homeAboutHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeActionHide();
            homeBlogInit();
            showPagePagination();
          } else if (index == 11) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHideFull();
            homeAboutHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionInit();
            showPagePagination();
          } else if (index == 12) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHideFull();
            homeAboutHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            showPagePagination();
            footerMenusHideAnimation();
          }
        },
        afterMove: function(index) {

        },
        loop: false,
        keyboard: true,
        responsiveFallback: 640,
        direction: "vertical"
      });

      // start intro animation on page load
      setTimeout(function () {
        if($('.onepage-pagination li .active').data('index') == 1) {
          homeIntroInit();
          fixedMenusInitAnimation();
        }
      }, 100);

    });

    var homeIntroReinit = true;
    var homeNavReinit = true;
    var homeThinkReinit = true;
    var homeVideoReinit = true;
    var homeAboutReinit = true;
    var homeFoundersReinit = true;
    var homeSystemReinit = true;
    var homeEventsReinit = true;
    var homeCollectionsReinit = false;
    var homeBlogReinit = true;
    var homeActionReinit = true;


    function homeIntroInit(){
      var logoTitle = '.intro-logo__title';
      var logoText = '.intro-logo__text';

      TweenMax.to($('.link-aside'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.3});
      TweenMax.to($('.scroll-down'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
      if ($(window).width() >= 640) {
        TweenMax.to($('.main-layout > .main-nav--socials'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
      }
      TweenMax.to($('.intro-logo'), 0, {autoAlpha: 1, ease: Power4.easeOut});
      TweenMax.to($('.intro-logo__circle--3'), 0.7, {autoAlpha: 1, ease: Power4.easeOut});
      TweenMax.to($('.intro-logo__circle--2'), 0.7, {
        autoAlpha: 1,
        top: '9.5%',
        left: '9.5%',
        width: '81%',
        height: '81%',
        ease: Power4.easeOut,
        delay: 0.5
      });
      TweenMax.to($('.intro-logo__circle--1'), 0.7, {
        autoAlpha: 1,
        top: 0,
        left: 0,
        width: '100%',
        height: '100%',
        ease: Power4.easeOut,
        delay: 1
      });
      TweenMax.to($('.intro-logo__circle'), 0.7, {autoAlpha: 0, ease: Power1.easeOut, delay: 3});
      TweenMax.to($('.intro-bg'), 3, {scaleX: 1.1, scaleY: 1.1, opacity: 1, ease: Power4.easeOut});
      TweenMax.to($('.intro-logo__img'), 2, {opacity: 1, delay: 0.5, ease: Power4.easeOut});
      TweenMax.to($(logoTitle), 0.7, {opacity: 1, delay: 1, ease: Power1.easeOut});
      TweenMax.to($(logoTitle), 2, {top: 0, delay: 1, ease: Power4.easeOut});
      TweenMax.to($(logoText), 0.7, {opacity: 1, delay: 1.5, ease: Power1.easeOut});
      TweenMax.to($(logoText), 2, {top: 0, delay: 1.5, ease: Power4.easeOut});

      homeIntroReinit = false;
    }

    function homeIntroHide(){
      if(!homeIntroReinit ) {
        var logoTitle = '.intro-logo__title';
        var logoText = '.intro-logo__text';

        //TweenMax.to($('.main-layout > .main-nav--socials'), 1, {opacity: 0, ease: Power4.easeOut});
        TweenMax.to($('.link-aside'), 1, {opacity: 0, ease: Power4.easeOut});
        TweenMax.to($('.scroll-down'), 1, {opacity: 0, ease: Power4.easeOut});

        TweenMax.to($('.intro-bg'), 3, {scaleX: 1, scaleY: 1, opacity: 0, ease: Power4.easeOut});
        TweenMax.to($('.intro-logo__circle--3'), 0.7, {autoAlpha: 0, ease: Power4.easeOut});
        TweenMax.to($('.intro-logo__circle--2'), 0.7, {
          autoAlpha: 0,
          top: '19%',
          left: '19%',
          width: '62%',
          height: '62%',
          ease: Power4.easeOut,
          delay: 0.5
        });
        TweenMax.to($('.intro-logo__circle--1'), 0.7, {
          autoAlpha: 0,
          top: '9.5%',
          left: '9.5%',
          width: '81%',
          height: '81%',
          ease: Power4.easeOut,
          delay: 1
        });
        TweenMax.to($('.intro-logo'), 1.7, {autoAlpha: 0, ease: Power4.easeOut});
        TweenMax.to($('.intro-logo__img'), 2, {opacity: 0, delay: 0.5, ease: Power4.easeOut});
        TweenMax.to($(logoTitle), 0.7, {opacity: 0, delay: 1, ease: Power1.easeOut});
        TweenMax.to($(logoTitle), 2, {top: '80%', delay: 1, ease: Power4.easeOut});
        TweenMax.to($(logoText), 0.7, {opacity: 0, delay: 1.5, ease: Power1.easeOut});
        TweenMax.to($(logoText), 2, {top: '80%', delay: 1.5, ease: Power4.easeOut});

        homeIntroReinit = true;
      }
    }

    function homeNavInit(){
      TweenMax.staggerTo( $(".home-nav__item"), 1, { top: '0', ease : Power1.easeInOut }, 0.2 );

      homeNavReinit = false;
    }

    function homeNavHide(){
      if(!homeNavReinit ) {
        TweenMax.staggerTo($(".home-nav__item"), 3, {top: '100%', ease: Power1.easeInOut}, 0.2);

        homeNavReinit = true;
      }
    }

    function homeThinkInit() {
      TweenMax.staggerTo($(".home-think__item-bg"), 0.8, {left: '0', opacity: '1', ease: Power1.easeInOut}, 0.5);
      TweenMax.staggerTo($(".home-think__item-title"), 0.8, {top: '0', opacity: '1', ease: Power1.easeInOut}, 0.5);
      TweenMax.staggerTo($(".home-think__item-text"), 0.8, {top: '0', opacity: '1', ease: Power1.easeInOut}, 0.5);
      homeVideoHide();

      TweenMax.to($('.home__circle--3'), 0.7, {
        autoAlpha: 0,
        top: '19%',
        left: '19%',
        width: '62%',
        height: '62%',
        ease: Power4.easeOut
      });
      TweenMax.to($('.home__circle--2'), 0.7, {
        autoAlpha: 0,
        top: '19%',
        left: '19%',
        width: '62%',
        height: '62%',
        ease: Power4.easeOut,
        delay: 0.1
      });
      TweenMax.to($('.home__circle--1'), 0.7, {
        autoAlpha: 0,
        top: '9.5%',
        left: '9.5%',
        width: '81%',
        height: '81%',
        ease: Power4.easeOut,
        delay: 0.2
      });

      homeThinkReinit = false;
    }

    function homeThinkHide() {
      if(!homeNavReinit ) {
        TweenMax.staggerTo($(".home-think__item-bg"), 0.8, {left: '-20%', opacity: '0', ease: Power1.easeInOut}, 0.5);
        TweenMax.staggerTo($(".home-think__item-title"), 0.8, {top: '40px', opacity: '0', ease: Power1.easeInOut}, 0.5);
        TweenMax.staggerTo($(".home-think__item-text"), 0.8, {top: '80px', opacity: '0', ease: Power1.easeInOut}, 0.5);

        homeThinkReinit = true;
      }
    }

    function homeVideoInit(){
      TweenMax.to($(".home-video__title"), 1.2, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.3});
      TweenMax.to($(".home-video__btn"), 1.2, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.6});
      TweenMax.to($(".home-video__text"), 1.2, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.9});
      TweenMax.to($('.home__circle-layout'), 0, {
        autoAlpha: 1
      });
      TweenMax.to($('.home__circle--3'), 0.7, {
        autoAlpha: 1,
        top: '19%',
        left: '19%',
        width: '62%',
        height: '62%',
        ease: Power4.easeOut,
        delay: 0.9
      });
      TweenMax.to($('.home__circle--2'), 0.7, {
        autoAlpha: 1,
        top: '9.5%',
        left: '9.5%',
        width: '81%',
        height: '81%',
        ease: Power4.easeOut,
        delay: 1.1
      });
      TweenMax.to($('.home__circle--1'), 0.7, {
        autoAlpha: 1,
        top: 0,
        left: 0,
        width: '100%',
        height: '100%',
        ease: Power4.easeOut,
        delay: 1.3
      });

      homeVideoReinit = false;
    }

    function homeVideoHide() {
      if(!homeVideoReinit ) {
        TweenMax.to($(".home-video__title"), 1.7, {top: '100px', opacity: '0', ease: Power1.easeInOut, delay: 0.3});
        TweenMax.to($(".home-video__btn"), 1.7, {top: '100px', opacity: '0', ease: Power1.easeInOut, delay: 0.3});
        TweenMax.to($(".home-video__text"), 1.7, {top: '100px', opacity: '0', ease: Power1.easeInOut, delay: 0.3});
        //TweenMax.to($('.home__circle--3'), 0.7, {
        //  autoAlpha: 0,
        //  top: '19%',
        //  left: '19%',
        //  width: '62%',
        //  height: '62%',
        //  ease: Power4.easeOut,
        //  delay: 0.5
        //});
        //TweenMax.to($('.home__circle--2'), 0.7, {
        //  autoAlpha: 0,
        //  top: '19%',
        //  left: '19%',
        //  width: '62%',
        //  height: '62%',
        //  ease: Power4.easeOut,
        //  delay: 0.6
        //});
        //TweenMax.to($('.home__circle--1'), 0.7, {
        //  autoAlpha: 0,
        //  top: '9.5%',
        //  left: '9.5%',
        //  width: '81%',
        //  height: '81%',
        //  ease: Power4.easeOut,
        //  delay: 0.7
        //});
        //TweenMax.to($('.home__circle-layout'), 0, {
        //  autoAlpha: 0,
        //  delay: 1.4
        //});

        homeVideoReinit = true;
      }
    }
    function homeVideoHideFull() {
      if (!homeVideoReinit) {
        TweenMax.to($(".home-video__title"), 1.7, {top: '100px', opacity: '0', ease: Power1.easeInOut, delay: 0.3});
        TweenMax.to($(".home-video__btn"), 1.7, {top: '100px', opacity: '0', ease: Power1.easeInOut, delay: 0.3});
        TweenMax.to($(".home-video__text"), 1.7, {top: '100px', opacity: '0', ease: Power1.easeInOut, delay: 0.3});
        TweenMax.to($('.home__circle--3'), 0.7, {
          autoAlpha: 0,
          top: '19%',
          left: '19%',
          width: '62%',
          height: '62%',
          ease: Power4.easeOut,
          delay: 0.5
        });
        TweenMax.to($('.home__circle--2'), 0.7, {
          autoAlpha: 0,
          top: '19%',
          left: '19%',
          width: '62%',
          height: '62%',
          ease: Power4.easeOut,
          delay: 0.6
        });
        TweenMax.to($('.home__circle--1'), 0.7, {
          autoAlpha: 0,
          top: '9.5%',
          left: '9.5%',
          width: '81%',
          height: '81%',
          ease: Power4.easeOut,
          delay: 0.7
        });
        TweenMax.to($('.home__circle-layout'), 0, {
          autoAlpha: 0,
          delay: 1.4
        });

        homeVideoReinit = true;
      }
    }


    function homeAboutInit(){
      TweenMax.to($(".home-about__logo"), 1, {scaleX: 1, scaleY: 1, opacity: '0.21', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.to($(".home-about__title"), 1, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.staggerTo($(".home-about-list__item"), 1, {opacity: '1', ease: Power4.easeInOut, delay: 0.5}, 0.2);
      TweenMax.staggerTo($(".home-about-list__item-number"), 1, {top: '0', ease: Power1.easeInOut, delay: 0.5}, 0.2);
      TweenMax.staggerTo($(".home-about-list__item-text"), 1, {top: '0', ease: Power1.easeInOut, delay: 0.5}, 0.2);
      TweenMax.staggerTo($(".home-about-counter__item"), 0.4, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 1.7}, 0.15);
      //var counter = { number1: 0, number2: 0, number3: 0, number4: 0, number5: 0 };
