extends ../layouts/main-client.jade

block contents
  div.main-layout#home
    include ../partials/main-nav-cities.jade
    include ../partials/main-nav-socials.jade
    include ../partials/main-nav-toggle.jade
    include ../partials/register-link.jade
    div.scroll-layout
      include ../partials/home/home-intro.jade
      include ../partials/home/home-nav.jade
      include ../partials/home/home-think.jade
      include ../partials/home/home-video.jade
      include ../partials/home/home-about.jade
      include ../partials/home/home-founders.jade
      include ../partials/home/home-system.jade
      include ../partials/home/home-events.jade
      include ../partials/home/home-collections.jade
      include ../partials/home/home-blog.jade
      include ../partials/home/home-action.jade
      include ../partials/footer.jade



  script.
    $(function () {
      fixedMenusInitAnimation();

      // founders hover anmation
      $('.home-founder').on('mouseover', function () {
        $('.home-founder').not(this).addClass('home-founder--disabled');
      });
      $('.home-founder').on('mouseleave', function () {
        $('.home-founder').not(this).removeClass('home-founder--disabled');
      });

      // hide fixed menus on mobile
      $(window).on('resize', function () {
        var window = $(this);
        if (window.width() < 640) {
          TweenMax.to($('.main-layout > .main-nav--cities'), 1, {opacity: 0, ease: Power1.easeOut});
          TweenMax.to($('.main-layout > .register-link'), 1, {opacity: 0, ease: Power1.easeOut});
          TweenMax.to($('.main-layout > .main-nav--socials'), 1, {opacity: 0, ease: Power1.easeOut});
          homeIntroInit();
          homeNavInit();
          homeThinkInit();
          homeVideoInit();
          homeAboutInit();
          homeFoundersInit();
          homeSystemInit();
          homeEventsInit();
          homeCollectionsInit();
          homeBlogInit();
          homeActionInit();
        } else {
          if ($('.onepage-pagination li .active').data('index') == 1) {
            TweenMax.to($('.main-layout > .main-nav--cities'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
            TweenMax.to($('.main-layout > .register-link'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
          }
        }
      });

      // init animations on mobile
      if ($(window).width() < 640) {
        homeIntroInit();
        homeNavInit();
        homeThinkInit();
        homeVideoInit();
        homeAboutInit();
        homeFoundersInit();
        homeSystemInit();
        homeEventsInit();
        homeCollectionsInit();
        homeBlogInit();
        homeActionInit();
      }

      // Onepage scroll init
      var pageScroll = ".scroll-layout";
      $(pageScroll).onepage_scroll({
        sectionContainer: "section",
        easing: "ease",
        animationTime: 2000,
        pagination: true,
        updateURL: false,
        beforeMove: function (index) {
          if (index == 12) {
            hideFixedMenus();
          } else {
            showFixedMenus();
          }

          if (index == 1) {
            homeNavHide();
            homeAboutHide();
            homeThinkHide();
            homeVideoHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeIntroInit();
          } else if (index == 2) {
            homeIntroHide();
            homeAboutHide();
            homeThinkHide();
            homeVideoHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeNavInit();
          } else if (index == 3) {
            homeIntroHide();
            homeNavHide();
            homeAboutHide();
            homeVideoHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeThinkInit();
          } else if (index == 4) {
            homeIntroHide();
            homeNavHide();
            homeAboutHide();
            homeThinkHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeVideoInit();
          } else if (index == 5) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeAboutInit();
          } else if (index == 6) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHide();
            homeAboutHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeFoundersInit();
          } else if (index == 7) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHide();
            homeAboutHide();
            homeFoundersHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeSystemInit();
          } else if (index == 8) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHide();
            homeAboutHide();
            homeFoundersHide();
            homeSystemHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionHide();
            homeEventsInit();
          }  else if (index == 9) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHide();
            homeAboutHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeBlogHide();
            homeActionHide();
            homeCollectionsInit();
          } else if (index == 10) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHide();
            homeAboutHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeActionHide();
            homeBlogInit();
          } else if (index == 11) {
            homeIntroHide();
            homeNavHide();
            homeThinkHide();
            homeVideoHide();
            homeAboutHide();
            homeFoundersHide();
            homeSystemHide();
            homeEventsHide();
            homeCollectionsHide();
            homeBlogHide();
            homeActionInit();
          }
        },
        afterMove: function(index) {

        },
        loop: false,
        keyboard: true,
        responsiveFallback: 640,
        direction: "vertical"
      });

      setTimeout(function () {
        if($('.onepage-pagination li .active').data('index') == 1) {
          homeIntroInit();
        }
      }, 100);


    });

    function showPagePagination() {
      setTimeout(function () {
        TweenMax.to($('.onepage-pagination'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.6});
      }, 100);
    }

    function fixedMenusInitAnimation() {
      if ($(window).width() >= 640) {
        TweenMax.to($('.main-nav--cities'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
        TweenMax.to($('.register-link'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
      }
      TweenMax.to($('.main-nav-toggle'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
    }

    function homeIntroInit(){
      var logoTitle = '.intro-logo__title';
      var logoText = '.intro-logo__text';

      TweenMax.to($('.link-aside'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.3});
      TweenMax.to($('.scroll-down'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
      if ($(window).width() >= 640) {
        TweenMax.to($('.main-layout > .main-nav--socials'), 1, {opacity: 1, ease: Power1.easeOut, delay: 0.7});
      }
      setTimeout(function () {
        TweenMax.to($('.onepage-pagination'), 1, {opacity: 0, ease: Power4.easeOut});
      }, 100);

      TweenMax.to($('.intro-bg'), 3, {scaleX: 1.1, scaleY: 1.1, opacity: 1, ease: Power4.easeOut});
      TweenMax.to($('.intro-logo__circle--3'), 0.7, {opacity: 1, ease: Power4.easeOut});
      TweenMax.to($('.intro-logo__circle--2'), 0.7, {opacity: 1, top: '9.5%', left: '9.5%', width: '81%', height: '81%', ease: Power4.easeOut, delay: 0.5});
      TweenMax.to($('.intro-logo__circle--1'), 0.7, {opacity: 1, top: 0, left: 0, width: '100%', height: '100%', ease: Power4.easeOut, delay: 1});
      TweenMax.to($('.intro-logo__img'), 2, {opacity: 1, delay: 0.5, ease: Power4.easeOut});
      TweenMax.to($(logoTitle), 0.7, {opacity: 1, delay: 1, ease: Power1.easeOut});
      TweenMax.to($(logoTitle), 2, {top: 0, delay: 1, ease: Power4.easeOut});
      TweenMax.to($(logoText), 0.7, {opacity: 1, delay: 1.5, ease: Power1.easeOut});
      TweenMax.to($(logoText), 2, {top: 0, delay: 1.5, ease: Power4.easeOut});
      TweenMax.to($('.intro-logo__circle'), 0.7, {opacity: 0, ease: Power1.easeOut, delay: 3});
    }

    function homeIntroHide(){
      var logoTitle = '.intro-logo__title';
      var logoText = '.intro-logo__text';

      TweenMax.to($('.main-layout > .main-nav--socials'), 1, {opacity: 0, ease: Power4.easeOut});
      TweenMax.to($('.link-aside'), 1, {opacity: 0, ease: Power4.easeOut});
      TweenMax.to($('.scroll-down'), 1, {opacity: 0, ease: Power4.easeOut});

      TweenMax.to($('.intro-bg'), 3, {scaleX: 1, scaleY: 1, opacity: 0, ease: Power4.easeOut});
      TweenMax.to($('.intro-logo__circle--3'), 0.7, {opacity: 0, ease: Power4.easeOut});
      TweenMax.to($('.intro-logo__circle--2'), 0.7, {
        opacity: 0,
        top: '19%',
        left: '19%',
        width: '62%',
        height: '62%',
        ease: Power4.easeOut,
        delay: 0.5
      });
      TweenMax.to($('.intro-logo__circle--1'), 0.7, {
        opacity: 0,
        top: '9.5%',
        left: '9.5%',
        width: '81%',
        height: '81%',
        ease: Power4.easeOut,
        delay: 1
      });
      TweenMax.to($('.intro-logo__img'), 2, {opacity: 0, delay: 0.5, ease: Power4.easeOut});
      TweenMax.to($(logoTitle), 0.7, {opacity: 0, delay: 1, ease: Power1.easeOut});
      TweenMax.to($(logoTitle), 2, {top: '80%', delay: 1, ease: Power4.easeOut});
      TweenMax.to($(logoText), 0.7, {opacity: 0, delay: 1.5, ease: Power1.easeOut});
      TweenMax.to($(logoText), 2, {top: '80%', delay: 1.5, ease: Power4.easeOut});
    }

    function homeNavInit(){
      TweenMax.staggerTo( $(".home-nav__item"), 1, { top: '0', ease : Power1.easeInOut }, 0.2 );
      showPagePagination();
    }

    function homeNavHide(){
      TweenMax.staggerTo($(".home-nav__item"), 3, {top: '100%', ease: Power1.easeInOut}, 0.2);
    }

    function homeThinkInit() {
      TweenMax.staggerTo($(".home-think__item-bg"), 0.8, {left: '0', opacity: '1', ease: Power1.easeInOut}, 0.5);
      TweenMax.staggerTo($(".home-think__item-title"), 0.8, {top: '0', opacity: '1', ease: Power1.easeInOut}, 0.5);
      TweenMax.staggerTo($(".home-think__item-text"), 0.8, {top: '0', opacity: '1', ease: Power1.easeInOut}, 0.5);
      showPagePagination();
    }

    function homeThinkHide() {
      TweenMax.staggerTo($(".home-think__item-bg"), 0.8, {left: '-20%', opacity: '0', ease: Power1.easeInOut}, 0.5);
      TweenMax.staggerTo($(".home-think__item-title"), 0.8, {top: '40px', opacity: '0', ease: Power1.easeInOut}, 0.5);
      TweenMax.staggerTo($(".home-think__item-text"), 0.8, {top: '80px', opacity: '0', ease: Power1.easeInOut}, 0.5);
    }

    function homeVideoInit(){
      TweenMax.to($(".home-video__title"), 1.7, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.3});
      TweenMax.to($(".home-video__btn"), 1.7, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.3});
      TweenMax.to($(".home-video__text"), 1.7, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.3});
      showPagePagination();
    }

    function homeVideoHide() {
      TweenMax.to($(".home-video__title"), 1.7, {top: '100px', opacity: '0', ease: Power1.easeInOut, delay: 0.3});
      TweenMax.to($(".home-video__btn"), 1.7, {top: '100px', opacity: '0', ease: Power1.easeInOut, delay: 0.3});
      TweenMax.to($(".home-video__text"), 1.7, {top: '100px', opacity: '0', ease: Power1.easeInOut, delay: 0.3});
    }

    function homeAboutInit(){
      TweenMax.to($(".home-about__title"), 1.2, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.staggerTo($(".home-about-list__item"), 1, {opacity: '1', ease: Power4.easeInOut, delay: 0.5}, 0.3);
      TweenMax.staggerTo($(".home-about-list__item-number"), 1, {top: '0', ease: Power1.easeInOut, delay: 0.5}, 0.3);
      TweenMax.staggerTo($(".home-about-list__item-text"), 1, {top: '0', ease: Power1.easeInOut, delay: 0.5}, 0.3);
      TweenMax.staggerTo($(".home-about-counter__item"), 0.4, {opacity: '1', ease: Power1.easeInOut, delay: 2.1}, 0.15);
      var counter = { number1: 0, number2: 0, number3: 0, number4: 0, number5: 0 };
      TweenMax.to(counter, 2, {
        number1: $('.home-about-counter__item:nth-child(1) .home-about-counter__item-number').text(),
        number2: $('.home-about-counter__item:nth-child(2) .home-about-counter__item-number').text(),
        number3: $('.home-about-counter__item:nth-child(3) .home-about-counter__item-number').text(),
        number4: $('.home-about-counter__item:nth-child(4) .home-about-counter__item-number').text(),
        number5: $('.home-about-counter__item:nth-child(5) .home-about-counter__item-number').text(),
        onUpdate: function () {
          $('.home-about-counter__item:nth-child(1) .home-about-counter__item-number').html(Math.ceil(counter.number1));
          $('.home-about-counter__item:nth-child(2) .home-about-counter__item-number').html(Math.ceil(counter.number2));
          $('.home-about-counter__item:nth-child(3) .home-about-counter__item-number').html(Math.ceil(counter.number3));
          $('.home-about-counter__item:nth-child(4) .home-about-counter__item-number').html(Math.ceil(counter.number4));
          $('.home-about-counter__item:nth-child(5) .home-about-counter__item-number').html(Math.ceil(counter.number5));
        },
        ease: Circ.easeOut,
        delay: 2.1
      });
      showPagePagination();
    }

    function homeAboutHide() {
      TweenMax.to($(".home-about__title"), 1.2, {top: '50%', opacity: '0', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.staggerTo($(".home-about-list__item"), 1, {opacity: '0', ease: Power4.easeInOut, delay: 0.5}, 0.3);
      TweenMax.staggerTo($(".home-about-list__item:nth-child(even) .home-about-list__item-number"), 1, {top: '-40px', ease: Power1.easeInOut, delay: 0.5}, 0.3);
      TweenMax.staggerTo($(".home-about-list__item:nth-child(even) .home-about-list__item-text"), 1, {top: '-40px', ease: Power1.easeInOut, delay: 0.5}, 0.3);
      TweenMax.staggerTo($(".home-about-list__item:nth-child(odd) .home-about-list__item-number"), 1, {top: '40px', ease: Power1.easeInOut, delay: 0.5}, 0.3);
      TweenMax.staggerTo($(".home-about-list__item:nth-child(odd) .home-about-list__item-text"), 1, {top: '40px', ease: Power1.easeInOut, delay: 0.5}, 0.3);
      TweenMax.staggerTo($(".home-about-counter__item"), 0.4, {opacity: '0', ease: Power1.easeInOut, delay: 2.1}, 0.15);
    }

    function homeFoundersInit() {
      TweenMax.staggerTo($(".home-founder:nth-child(odd) .home-founder__image"), 1.2, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.2}, 0.2);
      TweenMax.to($(".home-founder:nth-child(even) .home-founder__image"), 0.7, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.6});
      TweenMax.staggerTo($(".home-founder__preview-position"), 1, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 1}, 0.2);
      TweenMax.staggerTo($(".home-founder__preview-name"), 1, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 1.2}, 0.2);
      TweenMax.staggerTo($(".home-founder__preview-name-line"), 1.4, {bottom: '8px', opacity: '1', ease: Power1.easeInOut, delay: 1.6}, 0.2);
      TweenMax.staggerTo($(".home-founder__preview-star"), 0.4, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 2.4}, 0.2);
      showPagePagination();
    }

    function homeFoundersHide() {
      $('.home-founder__image').css({transition: 0});
      TweenMax.staggerTo($(".home-founder:nth-child(odd) .home-founder__image"), 1, {bottom: '20%', opacity: '0', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.staggerTo($(".home-founder:nth-child(even) .home-founder__image"), 1, {bottom: '40%', opacity: '0', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.staggerTo($(".home-founder__preview-position"), 1, {bottom: '-20px', opacity: '0', ease: Power1.easeInOut, delay: 1}, 0.2);
      TweenMax.staggerTo($(".home-founder__preview-name"), 1, {bottom: '-40px', opacity: '0', ease: Power1.easeInOut, delay: 1.2}, 0.2);
      TweenMax.staggerTo($(".home-founder__preview-name-line"), 1.4, {bottom: '28px', opacity: '0', ease: Power1.easeInOut, delay: 1.6}, 0.2);
      TweenMax.staggerTo($(".home-founder__preview-star"), 0.4, {bottom: '20px', opacity: '0', ease: Power1.easeInOut, delay: 2.4}, 0.2);
      showPagePagination();
    }

    function homeSystemInit() {
      TweenMax.to($(".page-title-big.home-system__title"), 1, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.to($('.home-system__logo'), 2, {scaleX: 1, scaleY: 1, opacity: 1, ease: Power4.easeOut, delay: 0.5});
      TweenMax.staggerTo($(".home-system-list__item:nth-child(1) .home-system-list__item-text"), 1.2, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.6}, 0.3);
      TweenMax.staggerTo($(".home-system-list__item:nth-child(2) .home-system-list__item-text"), 1.2, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.8}, 0.3);
      TweenMax.staggerTo($(".home-system-list__item:nth-child(1) .home-system-list__item-text-line"), 0.7, {right: '0', ease: Power1.easeInOut, delay: 1.5}, 0.3);
      TweenMax.staggerTo($(".home-system-list__item:nth-child(2) .home-system-list__item-text-line"), 0.7, {right: '0', ease: Power1.easeInOut, delay: 1.8}, 0.3);
      TweenMax.to($(".home-system__link"), 1.5, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.8});
      showPagePagination();
    }

    function homeSystemHide() {
      TweenMax.to($(".page-title-big.home-system__title"), 1, {bottom: '100px', opacity: '0', ease: Power1.easeInOut, delay: 0.3});
      TweenMax.to($('.home-system__logo'), 2, {scaleX: 0.75, scaleY: 0.6, opacity: 0, ease: Power1.easeOut, delay: 0.5});
      TweenMax.staggerTo($(".home-system-list__item:nth-child(1) .home-system-list__item-text"), 1.2, {bottom: '120px', opacity: '0', ease: Power1.easeInOut, delay: 0.6}, 0.3);
      TweenMax.staggerTo($(".home-system-list__item:nth-child(2) .home-system-list__item-text"), 1.2, {bottom: '-5px', opacity: '0', ease: Power1.easeInOut, delay: 0.8}, 0.3);
      TweenMax.staggerTo($(".home-system-list__item:nth-child(1) .home-system-list__item-text-line"), 0.7, {right: '100%', ease: Power1.easeInOut, delay: 1.5}, 0.3);
      TweenMax.staggerTo($(".home-system-list__item:nth-child(2) .home-system-list__item-text-line"), 0.7, {right: '100%', ease: Power1.easeInOut, delay: 1.8}, 0.3);
      TweenMax.to($(".home-system__link"), 1.5, {top: '30px', opacity: '10', ease: Power1.easeInOut, delay: 0.5});
    }

    function homeEventsInit() {
      TweenMax.to($(".page-title-big.home-events__title"), 1, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.to($(".home-events__more"), 1, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.staggerTo($(".home-events-list__item"), 1.2, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.5}, 0.2);
      TweenMax.staggerTo($(".home-events-list__item-date"), 1.2, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.5}, 0.2);
      TweenMax.staggerTo($(".home-events-list__item-duration"), 1.2, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.5}, 0.2);
      TweenMax.staggerTo($(".home-events-list__item-title"), 1.2, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.5}, 0.2);
      TweenMax.staggerTo($(".home-events-list__item-type"), 1.2, {opacity: '1', ease: Power1.easeInOut, delay: 0.5}, 0.2);
    }

    function homeEventsHide() {
      TweenMax.to($(".page-title-big.home-events__title"), 1, {top: '40px', opacity: '0', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.to($(".home-events__more"), 1, {top: '40px', opacity: '0', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.staggerTo($(".home-events-list__item"), 1.2, {bottom: '120px', opacity: '0', ease: Power1.easeInOut, delay: 0.5}, 0.2);
      TweenMax.staggerTo($(".home-events-list__item-date"), 1.2, {bottom: '60px', opacity: '0', ease: Power1.easeInOut, delay: 0.5}, 0.2);
      TweenMax.staggerTo($(".home-events-list__item-duration"), 1.2, {bottom: '60px', opacity: '0', ease: Power1.easeInOut, delay: 0.5}, 0.2);
      TweenMax.staggerTo($(".home-events-list__item-title"), 1.2, {bottom: '60px', opacity: '0', ease: Power1.easeInOut, delay: 0.5}, 0.2);
      TweenMax.staggerTo($(".home-events-list__item-type"), 1.2, {opacity: '0', ease: Power1.easeInOut, delay: 0.5}, 0.2);
    }

    function homeCollectionsInit() {
      TweenMax.to($(".home-collections__title-layout .home-collections__title"), 1, {bottom: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.to($(".home-collections__link"), 1, {top: '0', opacity: '1', ease: Power1.easeInOut, delay: 0.5});
      TweenMax.to($(".home-collections-list__item:nth-child(1), .home-collections-list__item:nth-child(2)"), 1.2, {left: '0%', ease: Power1.easeInOut, delay: 0.7});
      TweenMax.to($(".home-collections-list__item:nth-child(4), .home-collections-list__item:nth-child(5)"), 1.2, {right: '0%', ease: Power1.easeInOut, delay: 0.7});
    }

    function homeCollectionsHide() {
